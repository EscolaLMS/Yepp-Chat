{
  "name": "escolalms/yepp-chat",
  "description": "Escola Headless LMS Yepp Chat",
  "type": "package",
  "require": {
    "php": ">=7.4",
    "escolalms/auth": "^0",
    "escolalms/core": "^1",
    "escolalms/courses": "^0.4",
    "escolalms/settings": "^0",
    "escolalms/model-fields": "^0",
    "laravel/framework": ">=8.0"
  },
  "require-dev": {
    "orchestra/testbench": ">=5.0",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tomasz Smolarek",
      "email": "tomasz.smolarek@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\YeppChat\\": "src",
      "EscolaLms\\YeppChat\\Tests\\": "tests",
      "EscolaLms\\YeppChat\\Database\\Factories\\": "database/factories",
      "EscolaLms\\YeppChat\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\YeppChat\\EscolaLmsYeppChatServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
